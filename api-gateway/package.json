{
  "name": "appengine-typescript",
  "description": "Api Gateway - Movie App Microservices",
  "version": "0.0.1",
  "private": true,
  "license": "Apache Version 2.0",
  "author": "ccpinzon",
  "engines": {
    "node": ">=8.0.0"
  },
  "scripts": {
    "prepare": "npm run gcp-build",
    "pretest": "npm run gcp-build",
    "test": "repo-tools test app -- index.js",
    "posttest": "npm run lint",
    "lint": "tslint -p .",
    "start": "node ./index.js",
    "gcp-build": "tsc -p .",
    "appEngineDeploy": "npm run gcp-build && gcloud app deploy -v version4 --project=movieapp-microservices",
    "buildAndStart": "tsc -p . && ts-node ./index.ts"
  },
  "dependencies": {
    "express": "^4.18.2",
    "typescript": "^3.0.1",
    "axios": "^0.19.0",
    "dotenv": "^8.2.0",
    "ts-node": "^8.5.2",
    "cors": "latest"
  },
  "devDependencies": {
    "@google-cloud/nodejs-repo-tools": "^3.3.0",
    "@types/express": "^4.16.0",
    "tslint": "^5.11.0"
  }
}
